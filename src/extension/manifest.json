{
    "name": "Clickbait Time",
    "description": "Finds the thumbnail time of Youtube videos",
    "version": "1.0",
    "manifest_version": 2,
    "permissions":[
        "tabs",
        "https://imagematcher.herokuapp.com/",
        "https://youtubeaudio.majorcadevs.com/api/"
    ],
    "browser_action":{
        "default_popup": "popup/popup.html",
        "default_title": "test popup"
    },
    "background":{
        "page":"background/background.html"
    }

}